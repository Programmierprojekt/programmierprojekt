    "version": "0.2.0",
    "configurations": [
        {
            "name": "Chrome (Debug) with local backend",
            "request": "launch",
            "type": "dart",
            "args": ["-d", "chrome", "--web-browser-flag", "--disable-web-security"],
        },
        {
            "name": "Edge (Debug) with local backend",
            "request": "launch",
            "type": "dart",
            "args": ["-d", "Edge", "--web-browser-flag", "--disable-web-security"],
        },
        {
            "name": "programmierprojekt (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "programmierprojekt (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}